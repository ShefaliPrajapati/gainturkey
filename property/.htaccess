Options -Indexes

# Prod
RewriteEngine On
RewriteCond %{REQUEST_URI} !^/gainturkey/property
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ /index.php?$1 [L]


# Local/Demo
RewriteEngine On
RewriteCond %{REQUEST_URI} ^/gainturkey/property
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ /gainturkey/property/index.php?$1 [L]
